    rdfs:comment "Relation between a Chronology and a Chronology"@en ;
    rdfs:domain ecpo:Chronology ;
    rdfs:isDefinedBy <http://purl.org/ontology/ecpo> ;
    rdfs:label "has subchronology"@en, "hat untergeordneten Bestandsverlauf"@de ;
    rdfs:range ecpo:Chronology ;
    rdfs:subPropertyOf dcterms:hasPart .

[]
    a owl:Class ;
    owl:disjointWith [
        a owl:Class ;
        owl:intersectionOf (ecpo:Chronology
            [
                a owl:Restriction ;
                owl:onProperty ecpo:hasItemized ;
                owl:someValuesFrom owl:Thing
            ]
        )
    ] ;
    owl:intersectionOf (ecpo:Chronology
        [
            a owl:Class ;
            owl:unionOf ([
                    a owl:Restriction ;
                    owl:onProperty ecpo:hasBegin ;
                    owl:someValuesFrom owl:Thing
                ]
                [
                    a owl:Restriction ;
                    owl:onProperty ecpo:hasEnd ;
                    owl:someValuesFrom owl:Thing
                ]
            )
        ]
    ) .

[]
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ecpo:hasBegin ;
        owl:someValuesFrom owl:Thing
    ] ;
    owl:intersectionOf (ecpo:Chronlogy
        [
            a owl:Restriction ;
            owl:onProperty ecpo:hasEnd ;
            owl:someValuesFrom owl:Thing
        ]
    ) .

